{
  "name": "react-bs-datatable",
  "version": "3.0.0-alpha.1",
  "description": "React Bootstrap Datatable (without jQuery) with sorting, filter, and pagination",
  "main": "lib/cjs/index.js",
  "esnext": "lib/esm/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "typings": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "jest": "jest --watch",
    "build": "concurrently yarn:build:*",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.esm.json --emitDeclarationOnly --declaration --declarationDir lib/types",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "git@github.com:imballinst/react-bs-datatable.git",
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|ts)$"
  },
  "author": "Try Ajitiono",
  "license": "MIT",
  "homepage": "https://github.com/imballinst/react-bs-datatable#readme",
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "4.2.1",
    "@storybook/addon-actions": "6.4.14",
    "@storybook/addon-essentials": "6.4.14",
    "@storybook/addon-links": "6.4.14",
    "@storybook/react": "6.4.14",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.12",
    "@types/react": "17.0.38",
    "babel-jest": "24.1.0",
    "babel-loader": "8.2.3",
    "bootstrap": "5.1.3",
    "concurrently": "7.0.0",
    "emotion": "10.0.14",
    "font-awesome": "4.7.0",
    "jest": "27.4.7",
    "moment": "2.24.0",
    "react": "17.0.2",
    "react-bootstrap": "2.1.1",
    "react-dom": "17.0.2",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": ">= 17",
    "react-bootstrap": ">= 2.0.0",
    "react-dom": ">= 17"
  },
  "dependencies": {}
}
