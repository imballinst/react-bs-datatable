{
  "name": "react-bs-datatable",
  "version": "3.0.0-alpha.6",
  "description": "React Bootstrap Datatable (without jQuery) with sorting, filter, and pagination",
  "main": "lib/cjs/index.js",
  "esnext": "lib/esm/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "typings": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "jest": "jest --watch",
    "build": "rm -rf lib && concurrently yarn:build:*",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.esm.json --emitDeclarationOnly --declaration --declarationDir lib/types",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-story-data": "ts-node ./scripts/generate-story-data.ts",
    "preversion": "yarn test && rm -rf lib && yarn build",
    "v:alphamajor": "npm version premajor --preid alpha",
    "v:alphaminor": "npm version preminor --preid alpha",
    "v:alphapatch": "npm version prepatch --preid alpha",
    "v:prebump": "npm version prerelease"
  },
  "repository": "git@github.com:imballinst/react-bs-datatable.git",
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|ts)$"
  },
  "author": "Try Ajitiono",
  "license": "MIT",
  "homepage": "https://github.com/imballinst/react-bs-datatable#readme",
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.17",
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "4.2.1",
    "@storybook/addon-actions": "6.4.14",
    "@storybook/addon-essentials": "6.4.14",
    "@storybook/addon-links": "6.4.14",
    "@storybook/react": "6.4.14",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.12",
    "@types/react": "17.0.38",
    "babel-jest": "24.1.0",
    "babel-loader": "8.2.3",
    "bootstrap": "5.1.3",
    "concurrently": "7.0.0",
    "date-fns": "2.28.0",
    "emotion": "10.0.14",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-bootstrap": "2.1.1",
    "react-dom": "17.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.x",
    "@fortawesome/free-solid-svg-icons": "5.x",
    "@fortawesome/react-fontawesome": "0.x",
    "bootstrap": "5.x",
    "react": "17.x",
    "react-bootstrap": "2.x",
    "react-dom": "17.x"
  },
  "dependencies": {},
  "engines": {
    "node": "16"
  }
}
